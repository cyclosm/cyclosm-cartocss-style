- id: buildings
  <<: *extents
  Datasource:
    <<: *osm2pgsql
    table: |-
      (
        SELECT
          way,
          building AS type
        FROM planet_osm_polygon
        WHERE building IS NOT NULL
          AND building != 'no'
          AND way_area > 1*!pixel_width!::real*!pixel_height!::real
          AND (tags->'location' IS NULL OR tags->'location' != 'underground')
        ORDER BY z_order ASC, way_area DESC
      ) AS data
  geometry: polygon
  properties:
    minzoom: 16
- id: piers-poly
