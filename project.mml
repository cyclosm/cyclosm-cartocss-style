    table: |-
      (
        SELECT way, way_area AS area, COALESCE(landuse, leisure, "natural", highway, amenity) AS type
        FROM planet_osm_polygon
        WHERE way_area > 100000
        ORDER BY way_area DESC
      ) AS data
  geometry: multipolygon
  properties:
